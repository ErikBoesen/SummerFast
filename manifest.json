{
	"manifest_version": 2,
	"name": "SummerFast",
	"description": "Navigate through Virginia summer courses more easily",
	"version": "1.0",
	"icons": {
		"128": "128.png"
	},
	"content_scripts": [{
		"matches": ["https://scorm.schoology.com/*/screen.html"],
        "all_frames": true,
		"css": ["quiztools.css"],
		"js": ["quiztools.js"]
	}, {
		"matches": ["https://scorm.schoology.com/*"],
		"all_frames": true,
		"css": ["keyboardcontrol.css"],
		"js": ["keyboardcontrol.js"]
	}],
	"permissions": [
		"activeTab"
	]
}
